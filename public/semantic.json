{
  "base": "semantic/",
  "paths": {
    "source": {
      "config": "src/theme.config",
      "definitions": "src/definitions/",
      "site": "src/site/",
      "themes": "src/themes/"
    },
    "output": {
      "packaged": "out/",
      "uncompressed": "out/components/",
      "compressed": "out/components/",
      "themes": "out/themes/"
    },
    "clean": "dist/"
  },
  "permission": false,
  "rtl": "No",
  "components": [
    "button",
    "divider",
    "header",
    "icon",
    "image",
    "input",
    "label",
    "list",
    "segment",
    "grid",
    "menu",
    "card",
    "item",
    "dropdown",
    "rating",
    "site",
    "reset",
    "transition"
  ],
  "version": "2.1.4"
}